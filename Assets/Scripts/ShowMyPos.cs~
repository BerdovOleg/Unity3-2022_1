using System.Collections;
using System.Collections.Generic;
using UnityEngine;
using TMPro;

public class ShowMyPos : MonoBehaviour
{
    TextMeshPro Text;

    // Update is called once per frame
    void Update()
    {

        Text.text = transform.position.x + "," + transform.position.y + "," + transform.position.z;
    }
}
